set fenc=utf-8
set nobackup
set noswapfile
set showcmd


set number
set cursorline
set virtualedit=onemore
set smartindent
set showmatch
syntax on
set laststatus=2
filetype plugin indent on
set background=dark

set tabstop=2
set shiftwidth=2
set expandtab

set ignorecase
set smartcase
set incsearch
set wrapscan
set hlsearch
